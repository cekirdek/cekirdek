
@arch x86-64
@changed rax rdi rsi rdx rcx r11
@sideeffect 1
printhn(değer:rax) {
	@asm-data
"enter:
	.octa	0
	.byte	10";
	@asm-rodata
".hex:
	.string	""0123456789ABCDEF""";
	@asm
	"mov rcx, 0
döngü:
	rol değer, 4
	mov rdx, değer
	and rdx, 0x0F
	mov dil, [.hex+rdx]
	mov [enter+rcx], dil
	inc rcx
	cmp rcx, 16
	jnz döngü

	mov rdx, 17		# 
	lea	rsi, enter	# 
	mov rdi, 1		# file descriptor : 1 - standard output
	mov rax, 1		# system call : write (1)
	syscall			# Call the kernel
	ret";
}

